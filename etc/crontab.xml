<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Magento/Cron/etc/crontab.xsd">
    <group id="cronscheduler">
        <!-- heartbeat job - must be run every minute -->
        <job name="cronscheduler_heartbeat" instance="Wyomind\CronScheduler\Cron\HeartBeat" method="heartbeat">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>